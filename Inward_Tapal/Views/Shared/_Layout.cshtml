<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
   
    @Styles.Render("~/Content/css")
        @Styles.Render("~/bundles/MultiselectCss")
        <link href="~/Content/DatePicker/jquery-ui.css" rel="stylesheet" />
    @Styles.Render("~/Content/DatePickercss")
        @Styles.Render("~/Content/Autocompletecss")
        <link href="~/Content/fixedsidebar.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jqueryvalidation")
    @Scripts.Render("~/bundles/jquerynumeric")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/MultiselectJs")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AutoComplete")
        @Scripts.Render("~/bundles/DatePicker")
        <script src="~/Scripts/fixedSidebar.js"></script>

</head>
<body>
    <div class="navbar  navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse" style="background-color: #EEE8AA; ">
                <ul class="nav navbar-nav" style="padding-left:40px"  >

                    <li>@Html.ActionLink("New Entry", "Create", "Inward")</li>

                    <li>@Html.ActionLink("Edit", "EditTapal", "Inward")</li>

                    <li>@Html.ActionLink("Search", "Index", "Search")</li>
                    <li>@Html.ActionLink("Issued", "Issue", "Issuing")</li>
                    <li>@Html.ActionLink("Report", "Index", "Report")</li>
                </ul>
                @*@Html.Partial("_LoginPartial")*@

            </div>
            @*</table>*@
        </div>
    </div>
    @*<div class="container body-content">
        @RenderBody()
      
    </div>*@
    <div id="wrapper">

        @{Html.RenderPartial("_SidePanel");}
        <div id="main-content">
            <div class="container-fluid">
                <div class="col-lg-12">
                    @RenderBody()
                </div>
                @*<hr />
                    <footer>
                        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                    </footer>*@
            </div>
        </div>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
